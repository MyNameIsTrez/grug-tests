on_a() {
    v1: target = null_id
    set_target(v1)
    v2: target = null_id
    set_target(v2)
    v3: target = null_id
    set_target(v3)
    v4: target = null_id
    set_target(v4)
    v5: target = null_id
    set_target(v5)
    v6: target = null_id
    set_target(v6)
    v7: target = null_id
    set_target(v7)
    v8: target = null_id
    set_target(v8)
    v9: target = null_id
    set_target(v9)
    v10: target = null_id
    set_target(v10)
    v11: target = null_id
    set_target(v11)
    v12: target = null_id
    set_target(v12)
    v13: target = null_id
    set_target(v13)
    v14: target = null_id
    set_target(v14)

    # This causes SUB_RSP_32_BITS to be used in the function prologue,
    # instead of SUB_RSP_8_BITS, since there are now over 256 variable bytes
    v15: target = null_id
    set_target(v15)
}
